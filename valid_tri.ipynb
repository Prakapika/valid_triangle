    "def checkValidity(a, b, c):\n",
    "    if (a + b <= c) or (a + c <= b) or (b + c <= a) :\n",
    "        return False\n",
    "    else:\n",
    "        return True\t\n",
    "\n",
    "a = 2\n",
    "b = 4\n",
    "c = 2\n",
    "if checkValidity(a, b, c):\n",
    "    print(\"Valid Triangle\")\n",
    "else:\n",
    "    print(\"Valid Rectangle\")"
